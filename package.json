{
  "name": "@sore/xstyled",
  "version": "0.0.1",
  "description": "A simple xstyled UI library for React Project",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/*.css": "./dist/*.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && vite build",
    "lint": "eslint src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "ui-kit",
    "xstyled",
    "ui-library"
  ],
  "author": {
    "name": "Ashari Muhammad H",
    "email": "asharimh97@gmail.com",
    "url": "https://iamashari.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asharimh97/sore-xstyled.git"
  },
  "bugs": {
    "url": "https://github.com/asharimh97/sore-xstyled/issues"
  },
  "homepage": "https://github.com/asharimh97/sore-xstyled",
  "license": "ISC",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "storybook": "^8.1.11",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@xstyled/styled-components": "^4.0.0",
    "styled-components": "^6.1.11"
  }
